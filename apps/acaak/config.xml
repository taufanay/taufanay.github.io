<?xml version='1.0' encoding='utf-8'?>
<widget id="com.annexdesk.apps.acaak" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Acaak</name>
    <description>
        Acaaak is a simple Random Picker application.
        It that will pick up randomly one result from a given list.
        You can edit, save and load your list.
    </description>
    <author email="support+acaak@annexdesk.com" href="http://annexdesk.com">
        Annexdesk Team
    </author>
    <content src="index.html" />
    <hook src="scripts/hook_appversion.node.js" type="before_build" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="28" />
    <preference name="android-targetSdkVersion" value="29" />
</widget>
